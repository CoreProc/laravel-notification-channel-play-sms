{
    "name": "coreproc/laravel-notification-channel-play-sms",
    "description": ":package_description",
    "homepage": "https://github.com/CoreProc/laravel-notification-channel-play-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "CoreProc",
            "email": "ask@coreproc.com",
            "homepage": "https://coreproc.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "CoreProc\\PlaySms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoreProc\\PlaySms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CoreProc\\PlaySms\\PlaySmsServiceProvider"
            ]
        }
    }
}
